# Environment variables for Gemini Store Chatbox

# Google Gemini API Key (required)
GOOGLE_API_KEY=your_gemini_api_key_here

# Admin token for accessing admin panel (required)
ADMIN_TOKEN=your_secure_admin_token_here

# Server port (optional, default: 3000)
PORT=3000

# Database path (optional, default: ./data/chatbox.db)
DB_PATH=./data/chatbox.db

# Upload directory (optional, default: ./uploads)
UPLOAD_DIR=./uploads
